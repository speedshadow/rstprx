# Elite Rama Proxy Environment Configuration

# Server Configuration
LISTEN_ADDR=0.0.0.0:8443
TLS_MODE=selfsigned

# Security
JWT_SECRET=change_me_to_secure_random_string_minimum_32_characters
DEFAULT_ADMIN_USER=admin
DEFAULT_ADMIN_PASSWORD=change_me_secure_password

# Database
DATABASE_PATH=data/elite.db

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE=logs/proxy.log

# Features
ENABLE_PROMETHEUS=true
ENABLE_JA3_SPOOFING=true
ENABLE_JA4_SPOOFING=true
ENABLE_BEHAVIORAL_MIMICRY=true
ENABLE_CIRCUIT_BREAKER=true
ENABLE_RATE_LIMIT=true

# Development
RUST_LOG=info,rama_elite_proxy=debug
RUST_BACKTRACE=1
